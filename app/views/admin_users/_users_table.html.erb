<div class="panel-body">
  <%= render partial: "shared/table_filter_bar.html.erb", locals: { users: true } %>
  <div class="row">
    <div class="col-sm-12">
      <h3 class="medium">Users</h3>
      <table id="target-table-admin-users" class="target-table-admin-users data-table table" data-table-name='admin-users' data-can-export="<%= current_admin_user.export_abilities? %>">
        <thead>
          <tr>
            <% @headers.each do |header|%>
              <th class='column-<%= header %>'><%= header.humanize %></th>
            <% end %>
          </tr>
        </thead>
      </table>
    </div>
  </div>
</div>