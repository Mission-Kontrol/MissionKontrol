<div class="row wrapper border-bottom white-bg page-heading">
  <div class="col-lg-10">
    <h2><%= @current_table %></h2>

    <ol class="breadcrumb">
      <li>
        <span>Tables</span>
      </li>

      <li class="active">
        <strong><%= @current_table %></strong>
      </li>
    </ol>
  </div>
</div>

<div class="wrapper wrapper-content animated fadeInRight">
  <div class="row">
    <div class="col-lg-12">
      <div class="ibox float-e-margins">
        <div class="ibox-content">

          <br>

          <%= render 'table', hidden_columns: @hidden_columns, current_table: @current_table %>
        </div>
      </div>
    </div>
  </div>
</div>

<%= render 'column_settings_modal', columns: @headers, hidden_columns: @hidden_columns  %>
