<div class="row wrapper border-bottom white-bg page-heading">
  <div class="col-lg-10">
    <h2>Settings</h2>

    <ol class="breadcrumb">
      <li>
        <span>Settings</span>
      </li>

      <li class="active">
        <strong>Permissions</strong>
      </li>
    </ol>
  </div>
</div>

<div class="panel-body">
  <div class="row">
    <div class="col-sm-12">
      <table id="target-table-permissions" class="data-table table" data-table-name='permissions'>
        <thead>
          <tr>
            <% @headers.each do |header|%>
              <th class='column-<%= header %>'><%= header.humanize %></th>
            <% end %>
          </tr>
        </thead>
      </table>
    </div>
  </div>
</div>