<!DOCTYPE html>
<html>

  <head>
    <title>Kuwinda</title>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
    <%= tag :meta, name: :psj, action: action_name, controller: controller_name %>
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/v/bs/dt-1.10.18/b-1.5.6/b-colvis-1.5.6/b-html5-1.5.6/cr-1.5.0/r-2.2.2/rr-1.2.4/sc-2.0.0/sl-1.3.0/datatables.min.css"/>
  </head>

  <body>
    <div id="wrapper" class="<%= params[:controller] %>.<%= params[:action] %> ">
      <%= render 'layouts/navigation' %>
      <div id="page-wrapper" class="gray-bg <%= @extra_class %>">
        <%= render 'shared/flash_message' %>
        <%= render 'layouts/topnavbar' %>
        <%= yield %>
        <%= render 'layouts/footer' %>
      </div>
    </div>

    <div id="natterly-container"></div>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/simple-slider/1.0.0/simpleslider.min.js"></script>
    <%= render 'layouts/mouseflow' %>
    <%= render 'layouts/natterly' %>
    <%= render 'layouts/twilio' %>
    <%= render 'layouts/data_tables' %>
  </body>
</html>
