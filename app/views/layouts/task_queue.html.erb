<!DOCTYPE html>
<html>
  <head>
    <title>MissionKontrol</title>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
    <%= tag :meta, name: :psj, action: action_name, controller: controller_name %>
    <%= favicon_link_tag 'favicon.ico' %>
    <%= yield(:head) %>
  </head>

  <body class="gray-bg">
    <div class="container-fluid white-bg ">
      <div class="row">
        <div class='col-sm-12'>
          <%= render 'layouts/task_queue_topnavbar' %>
        </div>
      </div>
    </div>

    <div class="container-fluid">
      <div class="row" style="margin: 0">
        <div class="tab-content">
          <%= render 'layouts/task_queue_create_tab' %>
          <%= render 'layouts/task_queue_define_tab' %>
        </div>
      </div>

      <%= render 'shared/flash_message' %>

      <%= yield %>
      <%= insert_paloma_hook %>

      <% render 'layouts/footer' %>
    </div>

    <div id="natterly-container"></div>

    <%# render 'layouts/natterly' %>
  </body>
</html>
